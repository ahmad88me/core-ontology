###  http://postdata.linhd.uned.es/ontology/postdata-core#affects
<http://postdata.linhd.uned.es/ontology/postdata-core#affects> rdf:type owl:ObjectProperty ;
                                                               owl:inverseOf <http://postdata.linhd.uned.es/ontology/postdata-core#isAffectedBy> ;
                                                               rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Role> ;
                                                               rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Role> ;
                                                               rdfs:comment "This property points to an instance of Role who is affected by the current function (e.g. the role performed by an actress)"@en ;
                                                               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/POSTDATA-core/" ;
                                                               rdfs:label "affects"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel
<http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel> rdf:type owl:ObjectProperty ;
                                                                            rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#PoeticWork> ;
                                                                            rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                                                                       [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                                                                         owl:hasValue <http://postdata.linhd.uned.es/kos/author-educationLevel>
                                                                                       ] ;
                                                                            rdfs:comment "Level of education of the creator of a text/song (this property has as Domain \"PoeticWork\" and not \"Person\" since the education level of a person changes during time. This is the education level of the author in the moment of writing the poem)"@en ;
                                                                            rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/POSTDATA-core/" ;
                                                                            rdfs:label "authorEducationLevel"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-core#belongsToListOrderedText
