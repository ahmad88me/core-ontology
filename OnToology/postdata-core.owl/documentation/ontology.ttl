                                                                                rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#OrderedTextUnitList> ;
                                                                                rdfs:comment "The text unit belongs an ordered list of a textual units"@en ;
                                                                                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/POSTDATA-core/" ;
                                                                                rdfs:label "belongsToListOrderedText"@en .


